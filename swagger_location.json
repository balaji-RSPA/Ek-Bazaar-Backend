    "schemes": [
        "http","https"
    ],
    "basePath": "/api",
    "paths": {
        "/cities": {
            "get": {
                "summary": "Get cities",
                "description": "Get all the cities (For EKB Indian Cities will come through, for Onebazaar all the cities will come)",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "skip",
                        "description": "Number of cities which you want to skip (Optional)",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "in": "query",
                        "name": "limit",
                        "description": "Number of cities you want to Fetch (Optional)",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "in": "query",
                        "name": "search",
                        "description": "City name as search keyword(Optional)",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "in": "query",
                        "name": "stateId",
                        "description": "Required State ID's. This can be single String or an array of String",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "in": "query",
                        "name": "countryId",
                        "description": "Required country ID's. This can be single String or an array of String",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "success": {
                        "description": "Successful Response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "boolean"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "failed": {
                        "description": "Failed  Response -> Here Success value will be: 'false' with Status as 200",
                        "schema": {
                            "$ref": "#/definitions/InvalidResponse"
                        }
                    }
                }
            }
        },
        "/states": {
            "get": {
                "summary": "Get States",
                "description": "Get All Indian States",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "skip",
                        "description": "Skip State Count",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "in": "query",
                        "name": "limit",
                        "description": "Limit State Count",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "in": "query",
                        "name": "sellingStates",
                        "description": "Used While Adding Products (Any JS True Value)",
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "success": {
                        "description": "Successful Response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "boolean"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "failed": {
                        "description": "Failed  Response -> Here Success value will be: 'false' with Status as 200",
                        "schema": {
                            "$ref": "#/definitions/InvalidResponse"
                        }
                    }
                }
            }
        },
        "/countries": {
            "get": {
                "summary": "Get All Countries",
                "description": "Get All Countries",
                "produces": [
                    "application/json"
                ],
                "consumes": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "country",
                        "description": "Search with name (Optional)",
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "success": {
                        "description": "Successful Response",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "boolean"
                                },
                                "data": {
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "failed": {
                        "description": "Failed  Response -> Here Success value will be: 'false' with Status as 200",
                        "schema": {
                            "$ref": "#/definitions/InvalidResponse"
                        }
                    }
                }
            }
        }
    },
